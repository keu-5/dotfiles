{
  // github
  "github.copilot.nextEditSuggestions.enabled": true,

  // python
  "workbench.iconTheme": "material-icon-theme",
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "standard",

  // markdown
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },

  // prettier
  "prettier.enableDebugLogs": false,
  "editor.formatOnSave": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },

  // go
  "go.goroot": "/opt/homebrew/Cellar/go/1.25.1/libexec",
  "go.gopath": "/Users/cafe/go",
  "go.toolsEnvVars": {
    "PATH": "/opt/homebrew/bin:${env:PATH}"
  },

  // neovim
  "vscode-neovim.compositeKeys": {
    "jj": {
      "command": "vscode-neovim.escape"
    }
  },
  "keyboard.dispatch": "keyCode",
  "editor.fontFamily": "Hack Nerd Font Mono",

  // latex
  "[tex]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },

  "[latex]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },

  "[bibtex]": {
    "editor.tabSize": 2
  },

  "latex-workshop.intellisense.package.enabled": true,

  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.nav",
    "*.dvi",
    "*.synctex.gz"
  ],

  "latex-workshop.latex.outDir": "out",

  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk",
      "tools": ["latexmk"]
    }
  ],

  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": ["-silent", "-outdir=%OUTDIR%", "%DOC%"]
    }
  ],

  "latex-workshop.formatting.latexindent.path": "latexindent",
  "redhat.telemetry.enabled": true,
  "yaml.schemas": {
    "/Users/cafe/.vscode/extensions/continue.continue-1.0.20-darwin-arm64/config-yaml-schema.json": [
      ".continue/**/*.yaml"
    ]
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "latex-workshop.formatting.latex": "latexindent",
  "markdown.preview.breaks": true,
  "markdown-pdf.breaks": true,
  "git.suggestSmartCommit": false,
  "makefile.configureOnOpen": true,
  "git.ignoreRebaseWarning": true,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "git.openRepositoryInParentFolders": "never",

  "markdown.marp.html": "all",
  "database-client.autoSync": true,
  "cSpell.userWords": [
    "apikey",
    "appdb",
    "appendonly",
    "apppassword",
    "appuser",
    "coverprofile",
    "dbname",
    "entgo",
    "erroruser",
    "Errorw",
    "existinguser",
    "gofiber",
    "healthcheck",
    "IDEQ",
    "iloveyou",
    "imshow",
    "Infow",
    "interfacepkg",
    "isready",
    "letmein",
    "logomark",
    "minioadmin",
    "miniredis",
    "muzee",
    "mypassword",
    "newuser",
    "Nillable",
    "nosession",
    "npimg",
    "openapitools",
    "pycache",
    "randn",
    "randomhex",
    "redispassword",
    "resends",
    "Signup",
    "sqlc",
    "sslmode",
    "stretchr",
    "swaggo",
    "testset",
    "testuser",
    "trainloader",
    "trainset",
    "usecase",
    "userprofile",
    "venv",
    "vercel",
    "webp",
    "wrongpassword"
  ],
  "glassit.alpha": 240,
  "claudeCode.preferredLocation": "panel",
  "go.toolsManagement.autoUpdate": true,
  "claudeCode.selectedModel": "sonnet"
}
